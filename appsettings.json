{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "ConnectionString": "",
    "DatabaseName": "test"
  },
  "TaxUrl": "https://taxes.free.beeceptor.com/log-tax",
  "JwtSettings": {
    "Secret": "",
    "Issuer": "DuploBackend",
    "Audience": "DuploBackend"
  },
  "RateLimit": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "10m",
        "Limit": 100
      }
    ]
  }
}
